{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/game-day-2",
	"version": "0.1.0",
	"title": "Game Day 2",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "game-day-2",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"headline": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"kicker": {
			"type": "string",
			"source": "html",
			"selector": "h5"
		},
		"subdeck": {
			"type": "string",
			"source": "html",
			"selector": "h3"
		},
		"kickerBackgroundColor": {
			"type": "string",
			"default": "#4803BA"
		},
		"headlineColor": {
			"type": "string",
			"default": "#FF008C"
		}
	}
}
